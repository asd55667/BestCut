{
  "name": "bestcut",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "serve": "vite preview",
    "eslint": "eslint --cache --max-warnings 0 \"{src,plugins}/**/*.{vue,js,ts}\" --fix",
    "prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "stylelint": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "build": "vue-tsc --noEmit && vite build",
    "build:test": "vite build --mode test",
    "build:no-cache": "yarn clean:cache && npm run build",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "reinstall": "rimraf yarn.lock && rimraf package-lock.json && rimraf node_modules && yarn",
    "husky": "is-ci || husky install"
  },
  "dependencies": {
    "@vueuse/core": "^5.0.3",
    "ant-design-vue": "^2.2.7",
    "axios": "^0.21.1",
    "mockjs": "^1.1.0",
    "moment": "^2.29.1",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.0-rc.9",
    "vue": "^3.2.6",
    "vue-i18n": "9.1.6",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "@types/mockjs": "^1.0.4",
    "@types/node": "^16.3.1",
    "@types/lodash-es": "^4.17.4",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "@vitejs/plugin-legacy": "^1.4.4",
    "@vitejs/plugin-vue": "^1.6.1",
    "@vitejs/plugin-vue-jsx": "^1.1.6",
    "@vue/compiler-sfc": "^3.2.6",
    "autoprefixer": "^10.3.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.2",
    "less": "^4.1.1",
    "lint-staged": "^11.1.2",
    "postcss": "^8.0.9",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "tailwindcss": "^2.2.7",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.2",
    "vite": "^2.5.4",
    "vite-plugin-html": "^2.0.7",
    "vue-eslint-parser": "^7.8.0",
    "vue-tsc": "^0.2.2"
  }
}
