{
  "name": "chiulipine-monorepo",
  "version": "0.0.0",
  "scripts": {
    "dev": "run-p dev:editor dev:docs",
    "dev:editor": "pnpm -F editor dev",
    "dev:docs": "node scripts/autoExport & vitepress dev packages --port 3334",
    "lint:eslint": "eslint --cache --max-warnings 0 \"{src,plugins,mocks,tests}/**/*.{vue,js,ts,tsx}\" --fix",
    "lint:format": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:ls-lint": "ls-lint",
    "lint:lint-staged": "lint-staged",
    "clean:lib": "rimraf node_modules",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "build:test": "vite build --mode test",
    "build:no-cache": "pnpm clean:cache && pnpm build",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "test:unit": "vitest",
    "bootstrap": "pnpm install",
    "preinstall": "npx only-allow pnpm",
    "reinstall": "rimraf pnpm-lock.yaml && rimraf node_modules && pnpm bootstrap"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@vueuse/core": "^8.9.3",
    "ant-design-vue": "3.1.1",
    "axios": "^0.26.1",
    "gl-transitions": "^1.43.0",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "pinia": "2.0.12",
    "three": "^0.135.0",
    "uuid": "^8.3.2",
    "vue": "^3.2.37",
    "vue-i18n": "9.1.9",
    "vue-router": "^4.1.2"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.6",
    "@iconify-json/mdi": "^1.1.28",
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@ls-lint/ls-lint": "^1.11.2",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash-es": "^4.17.6",
    "@types/mockjs": "^1.0.6",
    "@types/node": "^16.11.45",
    "@types/three": "^0.135.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@unocss/reset": "^0.29.6",
    "@vitejs/plugin-legacy": "^1.8.2",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/theme": "^1.0.4",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^3.4.1",
    "fs-extra": "^10.1.0",
    "less": "^4.1.3",
    "lint-staged": "^12.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "~2.6.2",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.7.4",
    "unocss": "^0.41.2",
    "unplugin-auto-import": "^0.9.3",
    "unplugin-icons": "^0.14.7",
    "unplugin-vue-components": "^0.19.9",
    "vite": "^2.9.14",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-inspect": "^0.4.3",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-pages": "^0.25.0",
    "vitepress": "1.0.0-alpha.4",
    "vitest": "^0.16.0",
    "vue-eslint-parser": "^7.11.0",
    "vue-tsc": "^0.38.7"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false",
    "commit-msg": "pnpm exec node scripts/verifyCommit.js $1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "ls-lint",
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "ls-lint",
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,less,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup",
        "react",
        "react-dom",
        "@types/react",
        "@algolia/client-search"
      ]
    }
  }
}